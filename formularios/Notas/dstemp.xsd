<?xml version="1.0" standalone="yes"?>
<xs:schema id="dstemp" targetNamespace="http://www.tempuri.org/dstemp.xsd" xmlns:mstns="http://www.tempuri.org/dstemp.xsd" xmlns="http://www.tempuri.org/dstemp.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dstemp" msdata:IsDataSet="true" msdata:Locale="es-ES">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="notas">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:decimal" />
              <xs:element name="id_mat" type="xs:decimal" />
              <xs:element name="periodo" type="xs:string" />
              <xs:element name="materia" type="xs:string" />
              <xs:element name="nota" type="xs:decimal" minOccurs="0" />
              <xs:element name="profesora" type="xs:string" minOccurs="0" />
              <xs:element name="fecha_nota" type="xs:dateTime" minOccurs="0" />
              <xs:element name="generado" type="xs:dateTime" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:notas" />
      <xs:field xpath="mstns:id" />
    </xs:unique>
  </xs:element>
</xs:schema>