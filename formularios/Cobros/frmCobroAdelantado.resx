  <data name="SqlUpdateCommand1.CommandText" xml:space="preserve">
    <value>UPDATE cobros SET id_mat = @id_mat, carnet = @carnet, concepto = @concepto, mes = @mes, recibo = @recibo, fecha_recibo = @fecha_recibo, monto = @monto, generado = @generado, multa = @multa WHERE (id_cobro = @Original_id_cobro) AND (carnet = @Original_carnet) AND (concepto = @Original_concepto OR @Original_concepto IS NULL AND concepto IS NULL) AND (fecha_recibo = @Original_fecha_recibo OR @Original_fecha_recibo IS NULL AND fecha_recibo IS NULL) AND (generado = @Original_generado OR @Original_generado IS NULL AND generado IS NULL) AND (id_mat = @Original_id_mat) AND (mes = @Original_mes OR @Original_mes IS NULL AND mes IS NULL) AND (monto = @Original_monto OR @Original_monto IS NULL AND monto IS NULL) AND (multa = @Original_multa OR @Original_multa IS NULL AND multa IS NULL) AND (recibo = @Original_recibo OR @Original_recibo IS NULL AND recibo IS NULL); SELECT id_cobro, id_mat, carnet, concepto, mes, recibo, fecha_recibo, monto, generado, multa FROM cobros WHERE (id_cobro = @id_cobro)</value>
