  <data name="Estudiante.CommandText" xml:space="preserve">
    <value>SELECT matricula.carnet, matricula.id_mat, estudiantes.nombre + ' ' + estudiantes.apellido1 + ' ' + estudiantes.apellido2 AS nombre, niveles.cobros, matricula.nivel FROM matricula INNER JOIN estudiantes ON matricula.carnet = estudiantes.carnet INNER JOIN niveles ON matricula.nivel = niveles.nivel WHERE (matricula.carnet = @carnet) AND (matricula.ano = @ano)</value>
